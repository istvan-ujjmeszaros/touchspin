{
  "name": "@touchspin/jquery-plugin",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "jQuery wrapper for TouchSpin core (delegates to core, preserves API/events)",
  "license": "MIT",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite --port 8866 --strictPort",
    "build": "vite build",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@touchspin/core": "workspace:*"
  }
}
