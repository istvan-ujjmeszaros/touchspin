{
  "name": "@touchspin/renderer-bootstrap4",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Bootstrap 4 renderer for TouchSpin",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "src/index.ts",
  "scripts": {
