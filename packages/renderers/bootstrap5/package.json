{
  "name": "@touchspin/renderer-bootstrap5",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Bootstrap 5 renderer for TouchSpin",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/Bootstrap5Renderer.js",
      "require": "./src/Bootstrap5Renderer.js"
    }
  },
  "scripts": {
    "build:css": "sass src/styles/touchspin-bootstrap5.scss dist/touchspin-bootstrap5.css --style=expanded --source-map",
    "build": "npm run build:css",
    "watch:css": "sass src/styles/touchspin-bootstrap5.scss dist/touchspin-bootstrap5.css --style=expanded --source-map --watch"
  },
  "dependencies": {
    "@touchspin/core": "file:../../core"
  }
}

