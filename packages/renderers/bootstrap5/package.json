{
  "name": "@touchspin/renderer-bootstrap5",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Bootstrap 5 renderer for TouchSpin",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite --port 8866 --strictPort --open packages/renderers/bootstrap5/example/index.html",
    "build": "vite build",
    "watch:css": "sass src/styles/touchspin-bootstrap5.scss dist/touchspin-bootstrap5.css --style=expanded --source-map --watch"
  },
  "dependencies": {
    "@touchspin/core": "workspace:*"
  }
}
