{
  "name": "@touchspin/core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Core logic for TouchSpin (framework-agnostic)",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./dist/index.cjs"
    },
    "./renderer": {
      "types": "./src/renderer.ts",
      "import": "./src/renderer.ts",
      "require": "./dist/renderer.cjs"
    }
  },
  "scripts": {
    "dev": "vite --port 8866 --strictPort",
    "build": "vite build",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  }
}
